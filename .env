# Docker Compose can read environment variables from this file.
# See https://docs.docker.com/compose/env-file/

# Put admin areas behind a login prompt, with username and password
# specified here. Run `htpasswd -n admin` to create a password hash
# for user "admin". Paste the output here. SSL strongly recommended.
BASIC_AUTH=

# Let's Encrypt needs an email address for registration.
ACME_EMAIL=robert+millhousestrata.ca@mclellan.co

# The Traefik dashboard will be available at these domains.
# The URL is http://example.com/traefik/
TRAEFIK_DOMAINS=traefik.millhousestrata.ca
CLOUDFLARE_EMAIL=
CLOUDFLARE_API_KEY=
#JIRA
JIRA_DOMAINS=jira.millhousestrata.ca
JIRA_PROXY_NAME=jira.millhousestrata.ca
JIRA_PROXY_PORT=80
JIRA_PROXY_SCHEME=http
JIRA_CATALINA_OPTS=-Xms1g -Xmx3g -Datlassian.plugins.enable.wait=180
JIRA_DB_NAME=jiradb
JIRA_DB_USER=jira
JIRA_DB_PASSWORD=

#CONFLUENCE
CONFLUENCE_DOMAINS=wiki.millhousestrata.ca
CON_CATALINA_OPTS=-Xms1g -Xmx1g -Datlassian.plugins.enable.wait=180
CONFLUENCE_PROXY_NAME=wiki.millhousestrata.ca
CONFLUENCE_PROXY_PORT=80
CONFLUENCE_PROXY_SCHEME=http
CON_DB_NAME=confluencedb
CON_DB_USER=confluence
CON_DB_PASSWORD=
